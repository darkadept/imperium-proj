type User {
	id: Int
	firstName: String
	lastName: String
	email: String
	permissions: [Permission]
	roles: [Role]
	accounts: [Account]
}

type Permission {
	id: String
	name: String
}

type Role {
	id: Int
	name: String
	permissions: [Permission]
}

type Account {
	id: Int
	number: Int
	name: String
	permissions: [Permission]
	members: [User]
}

extend type Query {
	getUsers: [User]
}
